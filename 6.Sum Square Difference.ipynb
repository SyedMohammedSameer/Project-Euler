    "def compute():\n",
    "    N = 100\n",
    "    s = sum(i for i in range(1, N + 1))\n",
    "    s2 = sum(i**2 for i in range(1, N + 1))\n",
    "    return str(s**2 - s2)\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    print(compute())"
